<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
 		
        <record id="access_goufi_import_file_user" model="ir.model.access">
            <field name="name">access.goufi.import_file.user</field>
        	<field name="model_id" ref="model_goufi_import_file"/>
        	<field name="group_id" eval="ref('group_goufi_user')"/>
        	<field name="perm_read">1</field>
        	<field name="perm_write">1</field>
        	<field name="perm_create">0</field>
        	<field name="perm_unlink">0</field>
        </record>
        
        <record id="access_goufi_import_file_admin" model="ir.model.access">
            <field name="name">access.goufi.import_file.admin</field>
        	<field name="model_id" ref="model_goufi_import_file"/>
        	<field name="group_id" eval="ref('group_goufi_admin')"/>
        	<field name="perm_read">1</field>
        	<field name="perm_write">1</field>
        	<field name="perm_create">1</field>
        	<field name="perm_unlink">1</field>
        </record>
        
        
    
 		<!-- only admins should be able to define import configurations --> 
 		
        <record id="access_goufi_import_file_admin" model="ir.model.access">
            <field name="name">access.goufi.import_config.admin</field>
        	<field name="model_id" ref="model_goufi_import_configuration"/>
        	<field name="group_id" eval="ref('group_goufi_admin')"/>
        	<field name="perm_read">1</field>
        	<field name="perm_write">1</field>
        	<field name="perm_create">1</field>
        	<field name="perm_unlink">1</field>
        </record>
        
    </data>
</odoo>